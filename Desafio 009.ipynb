   "outputs": [],
   "source": [
    "t = int(input('Digite um número para exibir sua tabuada: '))\n",
    "print('-' * 12)\n",
    "for i in range(1,11):\n",
    "    print('{} x {:2} = {}'.format(t, i, i*t))\n",
    "print('-' * 12)"
   ]
